{
  "name": "jobflow",
  "version": "0.1.0",
  "private": true,
  "scripts": {
    "env:pull:dev": "doppler secrets download --no-file --format env > .env",
    "docker-build": "docker build -t git.local.guluarte.com/guluarte/jobflow-app:latest .",
    "docker-run": "docker run -it git.local.guluarte.com/guluarte/jobflow-app:latest",
    "docker-push": "docker push git.local.guluarte.com/guluarte/jobflow-app:latest",
    "docker-deploy": "npm run docker-build && npm run docker-push",
    "docker-services": "docker compose up",
    "dev": "next dev",
    "build": "npx prisma generate && npx prisma db push && npx next build",
    "format:write": "prettier --write \"**/*.{css,js,json,jsx,ts,tsx}\"",
    "format": "prettier \"**/*.{css,js,json,jsx,ts,tsx}\"",
    "start": "next start",
    "lint": "next lint",
    "db:pull": "npx prisma db pull && npx prisma generate",
    "db:push": "npx prisma generate && npx prisma db push",
    "storybook": "storybook dev -p 6006",
    "storybook:build": "storybook build",
    "email:preview": "email preview ./emails/",
    "stripe:listen": "stripe listen --forward-to http://localhost:3000/api/stripe"
